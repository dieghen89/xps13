# Mainteiner: dieghen89 <dieghen89 at gmail dot com>

pkgname="xps13-modules"
pkgver=1
pkgrel=1
pkgdesc="Load and blacklist modules for xps13"
arch=('i686' 'x86_64')
license=('custom')
source=("20-intel.conf")
sha256sums=('06781c6a651289f09ef5eb7c54cd3c739654d94d370f04d3ac3377a2bf6907b7')

package()
{
  cd "${srcdir}/"
  install -D -m644 i915.conf "${pkgdir}/etc/modprobe.d/i915.conf"
  install -D -m644 i2c_hid.conf "${pkgdir}/etc/modprobe.d/i2c_hid.conf"
}
